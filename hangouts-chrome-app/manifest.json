{
  "name": "Hangouts",
  "description": "Packaged app for Google Hangouts",
  "version": "1",
  "manifest_version": 2,
  "app": {
    "urls": [
      "*://hangouts.google.com/"
    ],
    "launch": {
      "web_url": "https://hangouts.google.com/"
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}